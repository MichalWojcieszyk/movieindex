<p id="notice"><%= notice %></p> 
<h1>List of questions</h1>

<table class="table">
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
      <th>Question</th>
      <th>Newsletter</th>
      <th>Created time</th>
    </tr>
  </thead>

  <tbody>
    <% @contacts.each do |contact| %>
      <tr>
        <td><%= contact.name %></td>
        <td><%= contact.email %></td>
        <td><%= contact.question %></td>
        <td><%= contact.newsletter %></td>
        <td><%= contact.created_at %></td>
      </tr>
    <% end %>
  </tbody>
</table>